<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.zerock.mapper.GoodsMapper">

	<!-- 	메서드명		 반환타입 -->
	<select id="getList" resultType="org.zerock.domain.GoodsVO"> 
		<![CDATA[
		select * from Shop_goods where goods_seq > 0
		]]>
	</select>
	
	<insert id="insert">
	insert into Shop_goods (goods_seq, gname, price, quantity) values (goods_seq.nextval, #{gname}, #{price}, #{quantity})
	</insert>
	
	<insert id="insertSelectKey">
	<!-- 190P -->
	
	
	</insert>
	
</mapper>